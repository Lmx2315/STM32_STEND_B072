        </StackPanel>
        <Button x:Name="button_adc0" Content="ADC0" HorizontalAlignment="Left" Margin="53,64,0,0" VerticalAlignment="Top" Width="75" Click="button_adc0_Click"/>
        <Button x:Name="button_adc1" Content="ADC1" HorizontalAlignment="Left" Margin="53,98,0,0" VerticalAlignment="Top" Width="75" Click="button_adc1_Click"/>
        <Button x:Name="button_dac0" Content="DAC0" HorizontalAlignment="Left" Margin="53,135,0,0" VerticalAlignment="Top" Width="75" Click="button_dac0_Click"/>
        <Button x:Name="button_dac1" Content="DAC1" HorizontalAlignment="Left" Margin="53,169,0,0" VerticalAlignment="Top" Width="75" Click="button_dac1_Click"/>
        <Button x:Name="button_console" Content="console" HorizontalAlignment="Left" Margin="219,119,0,0" VerticalAlignment="Top" Width="62" Click="button_Click_1"/>
        <Button x:Name="button_Panel" Content="Лиц. панель " HorizontalAlignment="Left" Margin="53,23,0,0" VerticalAlignment="Top" Width="75" Click="button_Click_2"/>
        <Button x:Name="button_init_lmk" Content="init LMK" HorizontalAlignment="Left" Margin="53,207,0,0" VerticalAlignment="Top" Width="75" Click="button_init_lmk_Click"/>

    </Grid>
</Window>
